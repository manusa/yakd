{
  "name": "yakd",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "ace-builds": "1.22.0",
    "ansi-to-html": "0.7.2",
    "dompurify": "3.0.3",
    "lodash": "4.17.21",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-ace": "10.1.0",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-router-dom": "6.1.1",
    "react-scripts": "5.0.1",
    "react-virtualized": "9.22.5",
    "redux": "4.1.2",
    "xterm": "4.15.0",
    "xterm-addon-attach": "0.6.0",
    "xterm-addon-fit": "0.5.0",
    "xterm-addon-web-links": "0.4.0",
    "yaml": "1.10.2"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "5.14.0",
    "autoprefixer": "9.8.6",
    "eslint-config-prettier": "8.8.0",
    "postcss-cli": "8.0.0",
    "prettier": "2.8.8",
    "tailwindcss": "1.9.6",
    "typeface-open-sans": "0.0.75"
  },
  "scripts": {
    "eslint": "eslint src/.",
    "prettier": "prettier 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}' --ignore-path ../../../.gitignore --write",
    "prestart": "npm run prettier && npm run eslint",
    "start": "npm run watch:css && react-scripts start",
    "prebuild": "npm run prettier && npm run eslint && npm run build:css",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "postcss src/assets/tailwind-src.css -o src/assets/tailwind.css",
    "watch:css": "postcss src/assets/tailwind-src.css -o src/assets/tailwind.css"
  },
  "eslintConfig": {
    "extends": ["react-app", "prettier"]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "arrowParens": "avoid",
    "bracketSpacing": false,
    "trailingComma": "none",
    "singleAttributePerLine": false
  }
}
